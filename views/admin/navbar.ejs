    <script>
        document.addEventListener("DOMContentLoaded", function () {

            const dropdowns = document.querySelectorAll(".custom-dropdown");

            dropdowns.forEach(dropdown => {
                const toggle = dropdown.querySelector(".dropdown-toggle-click");
                const menu = dropdown.querySelector(".dropdown-menu-click");

                // ðŸ”¹ Toggle dropdown ONLY when clicking header
                toggle.addEventListener("click", function (e) {
                    e.preventDefault();
                    e.stopPropagation();

                    dropdowns.forEach(d => {
                        if (d !== dropdown) {
                            d.classList.remove("open");
                            const m = d.querySelector(".dropdown-menu-click");
                            if (m) m.style.display = "none";
                        }
                    });

                    dropdown.classList.toggle("open");
                    menu.style.display = dropdown.classList.contains("open") ? "block" : "none";
                });

                // âœ… Allow links inside dropdown to work
                menu.querySelectorAll("a").forEach(link => {
                    link.addEventListener("click", function (e) {
                        e.stopPropagation(); // VERY IMPORTANT
                        // âŒ do NOT preventDefault here
                    });
                });
            });

            // optional: close dropdown when clicking outside
            document.addEventListener("click", () => {
                dropdowns.forEach(d => {
                    d.classList.remove("open");
                    const m = d.querySelector(".dropdown-menu-click");
                    if (m) m.style.display = "none";
                });
            });

        });
    </script>